{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#components/*": ["src/components/*"],
      "#layouts/*": ["src/layouts/*"],
      "#libraries/*": ["src/libraries/*"],
      "#services/*": ["src/services/*"],
      "#stores/*": ["src/stores/*"],
      "#styles/*": ["src/styles/*"],
      "#types/*": ["src/types/*"],
      "#utils/*": ["src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".eslintrc.cjs",
    "**/*.astro",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts", "**/*.tsx", 
  ],
}